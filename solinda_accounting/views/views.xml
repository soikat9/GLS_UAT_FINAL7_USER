<odoo>
  <data>

    <record id="invoice_bil_treesol" model="ir.ui.view">
      <field name="name">account.move.view.tree.solinherit</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_in_invoice_bill_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='state']" position="after">
          <button string="View PO" name="view_po_action" type="object" class="oe_highlight"/>

        </xpath>

      </field>
    </record>

    <record id="ref_origin_show_inherit_account_move" model="ir.ui.view">
      <field name="name">ref.orgin.view.out.account</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_in_invoice_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='invoice_origin']" position="attributes">
          <attribute name="optional">show</attribute>
        </xpath>
        <xpath expr="//field[@name='ref']" position="attributes">
          <attribute name="optional">show</attribute>
        </xpath>
        <field name="invoice_origin" position="after">
          <field name="total_qty_ordered" />
          <field name="total_qty_received" />
          <field name="product_type" />
        </field>
      </field>
    </record>

    <record id="account_inherit_account_move" model="ir.ui.view">
      <field name="name">account.inherit.move.view.out.</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_out_invoice_tree"/>
      <field name="arch" type="xml">
        <field name="invoice_date_due" position="after">
          <field name="total_qty_sale" />
          <field name="total_delivered_qty" />
        </field>
      </field>
    </record>

    <record id="account_move_form_inherit_qty_received" model="ir.ui.view">
      <field name="name">account.move.form.inherit.qty.received</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_move_form"/>
      <field name="arch" type="xml">
        <xpath expr="//header" position="inside">
          <button name="action_print_inv_boo" type="object" string="Print Invoice BOO" class="oe_highlight" attrs="{'invisible':[('print_button_visible','!=','boo')]}" />
          <button name="action_print_inv_oms" type="object" string="Print Invoice OMS" class="oe_highlight" attrs="{'invisible':[('print_button_visible','!=','oms')]}" />
          <button name="action_print_inv_turnkey" type="object" string="Print Invoice Turnkey" class="oe_highlight" attrs="{'invisible':[('print_button_visible','!=','turnkey')]}" />
          <button name="action_print_inv_trading" type="object" string="Print Invoice Trading" class="oe_highlight" attrs="{'invisible':[('print_button_visible','!=','trading')]}" />
        </xpath>
        <xpath expr="//group/group[@id='header_left_group']/field[@name='payment_reference']" position="attributes">
          <attribute name="invisible">1</attribute>
        </xpath>
        <xpath expr="//notebook/page/field/tree/field[@name='price_subtotal']" position="after">
          <field name="qty_received_account" />
        </xpath>
        <xpath expr="//notebook/page/field/tree/field[@name='qty_received_account']" position="after">
          <field name="qty_delivered_account" readonly="1"/>
        </xpath>
        <xpath expr="//field[@name='invoice_line_ids']/form/sheet/group[2]/field[@name='analytic_account_id']" position="attributes">
          <attribute name="required">1</attribute>
        </xpath>
        <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='analytic_account_id']" position="attributes">
          <attribute name="required">1</attribute>
        </xpath>
        <xpath expr="//notebook/page[@name='other_info']/group/group[@name='sale_info_group']/field[@name='partner_bank_id']" position="after">
          <field name="approved" />
          <field name="transfer"/>
          <field name="tags_ids" widget="many2many_tags" options="{'color_field': 'color'}" readonly="1"/>
          <field name="print_button_visible" invisible="1" />
          <!-- <field name="partner_bank_id" /> -->
        </xpath>
        <!-- <xpath expr="//notebook/page[@name='other_info']/group/group[@name='sale_info_group']/field[@name='ref']" position="attributes">
          <attribute name="required">1</attribute>
        </xpath> -->
      </field>
    </record>

    <record id="ournal_item_inherit_account" model="ir.ui.view">
      <field name="name">account.move.line.view.form.inherit</field>
      <field name="model">account.move.line</field>
      <field name="inherit_id" ref="account.view_move_line_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='analytic_account_id']" position="attributes">
          <attribute name="required">1</attribute>
        </xpath>
      </field>
    </record>

    <!-- <record id="account_payment_register_inherit_form" model="ir.ui.view">
      <field name="name">account.payment.register.form.inherit</field>
      <field name="model">account.payment.register</field>
      <field name="inherit_id" ref="account.view_account_payment_register_form"/>
      <field name="arch" type="xml">
        <field name="payment_date" position="before">
          <field name="other_costs"/>
        </field>
        <field name="other_costs" position="before">
          <field name="other_account"/>
        </field>
      </field>
    </record> -->

  </data>
</odoo>